<!--设置返回键的点击效果-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
    <shape>
        <corners android:radius="20dp" />
        <!--设置填充色-->
        <solid android:color="#C0C0C0"/>
    </shape>
</item>
<item>
    <shape>
        <corners android:radius="8dp" />
        <!--设置填充色-->
        <solid android:color="#F4F4F8"/>
    </shape>
</item>
</selector>